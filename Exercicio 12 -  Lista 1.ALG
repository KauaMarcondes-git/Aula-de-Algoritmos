Algoritmo "semnome"

Var
n1, n2, n3, total:inteiro


Inicio

      escreval("Qual o valor do número 1?")
      leia(n1)
      escreval("Qual o valor do número 2?")
      leia(n2)
      escreval("Qual o valor do número 3?")
      leia(n3)
      total<- n1*n2*n3
      escreva("O resultado final é ", total)


Fimalgoritmo